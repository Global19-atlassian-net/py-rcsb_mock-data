{
   "title": "Schema for content type repository_holdings collection repository_holdings_removed_v0_1", 
   "type": "object", 
   "properties": {
      "entry_id": {
         "type": "string", 
         "description": "The PDB entry accession code."
      }, 
      "title": {
         "type": "string", 
         "description": "The entry title."
      }, 
      "audit_authors": {
         "type": "array", 
         "uniqueItems": false, 
         "items": {
            "type": "string", 
            "examples": [
               "Bleary, Percival R.;O'Neil, F.K."
            ], 
            "description": "Names of the authors of the entry."
         }
      }, 
      "remove_date": {
         "type": "string", 
         "description": "The date the entry was removed from the PDB repository.", 
         "format": "date"
      }, 
      "deposit_date": {
         "type": "string", 
         "description": "The entry deposition date.", 
         "format": "date"
      }, 
      "release_date": {
         "type": "string", 
         "description": "The initial date the entry was released in the PDB repository.", 
         "format": "date"
      }, 
      "update_id": {
         "type": "string", 
         "description": "Identifier for the current data exchange status record."
      }, 
      "details": {
         "type": "string", 
         "description": "The entry title."
      }, 
      "id_codes_replaced_by": {
         "type": "array", 
         "uniqueItems": false, 
         "items": {
            "type": "string", 
            "description": "The list of superseding PDB accession codes for the removed entry."
         }
      }
   }, 
   "$id": "https://github.com/rcsb/py-rcsb.db/tree/master/rcsb.db/data/json-schema/", 
   "$schema": "http://json-schema.org/draft-04/schema#", 
   "required": [
      "entry_id", 
      "update_id"
   ]
}