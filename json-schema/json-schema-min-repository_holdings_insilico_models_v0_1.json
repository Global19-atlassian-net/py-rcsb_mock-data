{
   "title": "Schema for content type repository_holdings collection repository_holdings_insilico_models_v0_1", 
   "type": "object", 
   "properties": {
      "entry_id": {
         "type": "string", 
         "description": "The PDB entry accession code."
      }, 
      "title": {
         "type": "string", 
         "description": "The entry title."
      }, 
      "audit_authors": {
         "type": "string", 
         "examples": [
            "Bleary, Percival R., O'Neil, F.K."
         ], 
         "description": "Names of the authors of the entry."
      }, 
      "remove_date": {
         "type": "string", 
         "description": "The removal date for the entry.", 
         "format": "date"
      }, 
      "deposit_date": {
         "type": "string", 
         "description": "The entry deposition date.", 
         "format": "date"
      }, 
      "release_date": {
         "type": "string", 
         "description": "The release date for the entry.", 
         "format": "date"
      }, 
      "status_code": {
         "type": "string", 
         "description": "The release status for the entry.", 
         "enum": [
            "OBS", 
            "TRSF", 
            "WDRN"
         ]
      }, 
      "update_id": {
         "type": "string", 
         "description": "Identifier for the current data exchange status record."
      }, 
      "id_codes_replaced_by": {
         "type": "array", 
         "uniqueItems": false, 
         "items": {
            "type": "string", 
            "description": "The list of superseding PDB accession codes for the removed insilico model entry."
         }
      }
   }, 
   "$id": "https://github.com/rcsb/py-rcsb.db/tree/master/rcsb.db/data/json-schema/", 
   "$schema": "http://json-schema.org/draft-04/schema#", 
   "required": [
      "entry_id", 
      "update_id"
   ]
}